{
  "name": "vscode-tab-traversal",
  "displayName": "Tab Traversal",
  "publisher": "aleclarson",
  "description": "Slightly intelligent text traversal with the Tab key",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jumpForward"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jumpForward",
        "title": "Jump Forward"
      },
      {
        "command": "jumpBackward",
        "title": "Jump Backward"
      }
    ],
    "keybindings": [
      {
        "key": "tab",
        "command": "jumpForward",
        "when": "editorTextFocus && !inSnippetMode && !inlineSuggestionVisible && !suggestWidgetVisible"
      },
      {
        "key": "shift+tab",
        "command": "jumpBackward",
        "when": "editorTextFocus && !inSnippetMode"
      }
    ]
  },
  "prettier": "@alloc/prettier-config",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p .",
    "test": "vitest"
  },
  "devDependencies": {
    "@alloc/prettier-config": "^1.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@vscode/test-electron": "^2.0.3",
    "endent": "^2.1.0",
    "prettier": "^2.7.1",
    "typescript": "^4.5.4",
    "vitest": "^0.23.4"
  }
}
