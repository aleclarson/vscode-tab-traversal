{
  "name": "vscode-tab-traversal",
  "displayName": "Tab Traversal",
  "publisher": "aleclarson",
  "description": "Slightly intelligent text traversal with the Tab key",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.64.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:jumpForward"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jumpForward",
        "title": "Jump Forward"
      },
      {
        "command": "jumpBackward",
        "title": "Jump Backward"
      }
    ],
    "keybindings": [
      {
        "key": "tab",
        "command": "jumpForward",
        "when": "editorTextFocus && !inSnippetMode && !inlineSuggestionVisible && !suggestWidgetVisible"
      },
      {
        "key": "shift+tab",
        "command": "jumpBackward",
        "when": "editorTextFocus && !inSnippetMode"
      }
    ]
  },
  "prettier": "@alloc/prettier-config",
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@alloc/prettier-config": "^1.0.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.64.0",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vscode/test-electron": "^2.0.3",
    "endent": "^2.1.0",
    "eslint": "^8.6.0",
    "glob": "^7.2.0",
    "mocha": "^9.1.3",
    "prettier": "^2.7.1",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "vitest": "^0.23.4",
    "webpack": "^5.66.0",
    "webpack-cli": "^4.9.1"
  }
}
